{
  "type": "Dilemma",
  "dilemmaType": "Space",
  "cost": 2,
  "title": "Wavefront",
  "abilities": [
    {
      "text": "Unless you have a personnel who has 2 Astrometrics or a personnel who has 2 Navigation, your opponent chooses an Astrometrics or Navigation personnel to be stopped. If your opponent cannot, all your personnel are stopped and this dilemma returns to its owner's dilemma pile.",
      "type": "dilemmaEffect",
      "effect": [
        {
          "type": "condition",
          "condition": "or(countAtLeast(skill(Astrometrics),2),countAtLeast(skill(Navigation),2))",
          "falseEffect": {
            "type": "condition",
            "condition": "onMission(currentMission),or(skill(Astrometrics),skill(Navigation))",
            "trueEffect": [
              {
                "type": "opponentSelect",
                "filter": "onMission(currentMission),or(skill(Astrometrics),skill(Navigation))",
                "memorize": "selectedCharacter"
              },
              {
                "type": "stop",
                "filter": "memory(selectedCharacter)"
              }
            ],
            "falseEffect": [
              {
                "type": "stop",
                "filter": "onMission(currentMission),type(personnel)"
              },
              {
                "type": "putDilemmaInDilemmaPile",
                "filter": "self"
              }
            ]
          }
        }
      ]
    }
  ],
  "lore": "\"You can tell Baran that I'm working as fast as I can.\""
}
