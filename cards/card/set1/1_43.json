{
  "type": "Dilemma",
  "dilemmaType": "Dual",
  "cost": 2,
  "title": "Pinned Down",
  "abilities": [
    {
      "text": "Randomly select a personnel to be stopped. If you still have nine personnel remaining, randomly select a second personnel to be stopped. If you still have ten personnel remaining, randomly select a third personnel to be stopped.",
      "type": "dilemmaEffect",
      "effects": [
        {
          "type": "sequence",
          "actions": [
            {
              "type": "randomlySelect",
              "filter": "attemptingPersonnel,type(Personnel)",
              "memory": "selectedPersonnel1"
            },
            {
              "type": "stop",
              "filter": "memory(selectedPersonnel1)"
            },
            {
              "type": "condition",
              "condition": "countAtLeast(and(attemptingPersonnel,type(Personnel)),9)",
              "trueEffect": {
                "type": "sequence",
                "actions": [
                  {
                    "type": "randomlySelect",
                    "filter": "attemptingPersonnel,type(Personnel)",
                    "memory": "selectedPersonnel2"
                  },
                  {
                    "type": "stop",
                    "filter": "memory(selectedPersonnel2)"
                  },
                  {
                    "type": "condition",
                    "condition": "countAtLeast(and(attemptingPersonnel,type(Personnel)),10)",
                    "trueEffect": {
                      "type": "sequence",
                      "actions": [
                        {
                          "type": "randomlySelect",
                          "filter": "attemptingPersonnel,type(Personnel)",
                          "memory": "selectedPersonnel3"
                        },
                        {
                          "type": "stop",
                          "filter": "memory(selectedPersonnel3)"
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      ]
    }
  ],
  "lore": "\"Quick! Into the forest!\""
}
